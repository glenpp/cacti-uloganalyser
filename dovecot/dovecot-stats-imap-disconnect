PATH=/bin:/usr/bin
STATS=/var/local/snmp/mail


printf "%d\n" `grep ^dovecot:imap:connclosed= $STATS | cut -d '=' -f2`
printf "%d\n" `grep ^dovecot:imap:disconnect= $STATS | cut -d '=' -f2`
printf "%d\n" `grep ^dovecot:imap:disconnect:other= $STATS | cut -d '=' -f2`
printf "%d\n" `grep ^dovecot:imap:disconnect:inactivity= $STATS | cut -d '=' -f2`
printf "%d\n" `grep ^dovecot:imap:disconnect:loggedout= $STATS | cut -d '=' -f2`
printf "%d\n" `grep ^dovecot:imap:disconnect:idle= $STATS | cut -d '=' -f2`
